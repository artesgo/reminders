{
	"name": "Artesgo Reminders",
	"description": "TLDR: Multi-purpose URL scheduler + reminder extension. Schedule [insert website] urls to open exactly on time.",
	"version": "1.5",
	"manifest_version": 3,
	"permissions": ["storage", "alarms", "notifications", "contextMenus"],
	"action": {
		"default_popup": "index.html"
	},
	"background": {
		"service_worker": "background.js"
	},
	"icons": {
		"16": "icon/arrow-down--16--blue.png",
		"48": "icon/arrow-down--48--blue.png",
		"128": "icon/arrow-down--128--blue.png"
	}
}
